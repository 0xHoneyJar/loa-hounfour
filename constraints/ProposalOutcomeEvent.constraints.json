{
  "$schema": "https://loa-hounfour.dev/schemas/constraint-file.json",
  "schema_id": "ProposalOutcomeEvent",
  "origin": "genesis",
  "constraints": [
    {
      "id": "outcome-event-actor-required",
      "expression": "len(actor_id) >= 1",
      "severity": "error",
      "type_signature": {
        "actor_id": "string"
      },
      "fields": [
        "actor_id"
      ],
      "message": "Outcome events must have a non-empty actor ID."
    }
  ]
}
