{
  "$schema": "https://loa-hounfour.dev/schemas/constraint-file.json",
  "schema_id": "ConstraintCandidate",
  "origin": "genesis",
  "contract_version": "7.6.0",
  "expression_version": "1.0",
  "constraints": [
    {
      "id": "candidate-expression-nonempty",
      "expression": "expression != ''",
      "severity": "error",
      "fields": [
        "expression"
      ],
      "type_signature": {
        "expression": "string"
      },
      "message": "Constraint expression must be non-empty."
    },
    {
      "id": "candidate-fields-nonempty",
      "expression": "fields.length > 0",
      "severity": "error",
      "fields": [
        "fields"
      ],
      "type_signature": {
        "fields": "string[]"
      },
      "message": "Fields array must have at least one entry."
    }
  ]
}
