{
  "schema_id": "ConstraintCandidate",
  "origin": "genesis",
  "contract_version": "7.6.0",
  "expression_version": "1.0",
  "constraints": [
    {
      "id": "candidate-expression-nonempty",
      "description": "Constraint expression must be non-empty.",
      "expression": "expression != ''",
      "severity": "error",
      "fields": [
        "expression"
      ],
      "type_signature": {
        "expression": "string"
      }
    },
    {
      "id": "candidate-fields-nonempty",
      "description": "Fields array must have at least one entry.",
      "expression": "fields.length > 0",
      "severity": "error",
      "fields": [
        "fields"
      ],
      "type_signature": {
        "fields": "string[]"
      }
    }
  ]
}
