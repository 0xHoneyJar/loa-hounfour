{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.0xhoneyjar.com/loa-hounfour/8.0.0/event-cursor",
  "additionalProperties": false,
  "description": "Cursor for event replay and pagination. Specify at most one of after_event_id or after_timestamp.",
  "type": "object",
  "properties": {
    "after_event_id": {
      "format": "uuid",
      "description": "Resume from after this event ID (exclusive).",
      "type": "string"
    },
    "after_timestamp": {
      "format": "date-time",
      "description": "Resume from after this timestamp (exclusive).",
      "type": "string"
    },
    "limit": {
      "minimum": 1,
      "maximum": 1000,
      "default": 100,
      "description": "Maximum events per delivery batch.",
      "type": "integer"
    }
  },
  "$comment": "contract_version=8.0.0, min_supported=6.0.0"
}
