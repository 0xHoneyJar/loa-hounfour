        {
          "const": "cold",
          "type": "string"
        },
        {
          "const": "warming",
          "type": "string"
        },
        {
          "const": "established",
          "type": "string"
        },
        {
          "const": "authoritative",
          "type": "string"
        }
      ]
    },
    "at": {
      "format": "date-time",
      "type": "string"
    },
    "trigger": {
      "minLength": 1,
      "type": "string"
    }
  },
  "$id": "https://schemas.0xhoneyjar.com/loa-hounfour/7.9.0/reputation-transition",
  "$comment": "contract_version=7.9.0, min_supported=6.0.0"
