    "from": {
      "$id": "ReputationState",
      "anyOf": [
        {
          "const": "cold",
          "type": "string"
        },
        {
          "const": "warming",
          "type": "string"
        },
        {
          "const": "established",
          "type": "string"
        },
        {
          "const": "authoritative",
          "type": "string"
        }
      ]
    },
    "to": {
