{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.0xhoneyjar.com/loa-hounfour/8.3.0/transition-result",
  "additionalProperties": false,
  "description": "Result of a GovernedResource state transition.",
  "type": "object",
  "required": [
    "success",
    "new_state",
    "version"
  ],
  "properties": {
    "success": {
      "type": "boolean"
    },
    "new_state": {
      "description": "The state after transition (type varies per resource)."
    },
    "version": {
      "minimum": 0,
      "type": "integer"
    },
    "violations": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "$comment": "contract_version=8.3.0, min_supported=6.0.0"
}
